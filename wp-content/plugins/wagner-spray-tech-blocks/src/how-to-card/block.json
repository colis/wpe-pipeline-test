{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wst-blocks/how-to-card",
	"title": "How To Card",
	"category": "wst-blocks",
	"icon": "index-card",
	"description": "Card for displaying How To content.",
	"keywords": [ "card", "content", "how to", "video" ],
	"textdomain": "wagner-spray-tech-blocks",
	"parent": [ "wst-blocks/content-selector" ],
	"attributes": {
		"selectedPostType": {
			"type": "string",
			"default": ""
		},
		"post": {
			"type": "object",
			"default": {}
		}
	},
	"supports": {
		"align": false
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php"
}
