{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "useRootPaddingAwareAlignments": false,
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "color": {
      "background": false,
      "text": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "gradients": [],
      "link": false,
      "palette": []
    },
    "custom": {
      "colors": {
        "black": "#000",
        "black-rgba-01": "rgba(0, 0, 0, 0.1)",
        "black-rgba-0125": "rgba(0, 0, 0, 0.125)",
        "black-rgba-015": "rgba(0, 0, 0, 0.15)",
        "black-rgba-05": "rgba(0, 0, 0, 0.5)",
        "black-rgba-085": "rgba(0, 0, 0, 0.85)",
        "white": "#fff",
        "white-rgba-01": "rgba(255, 255, 255, 0.1)",
        "white-rgba-07": "rgba(255, 255, 255, 0.7)",
        "gray-100": "#f5f5f5",
        "gray-200": "#eeeeee",
        "gray-300": "#dddddd",
        "gray-400": "#333333",
        "yellow": "#FFCB00",
        "yellow-hover": "#FFE16A",
        "green": "rgb(30,94,49)",
        "red": "rgb(167,28,32)"
      },
      "grid": {
        "gutter": "24px",
        "width": "1520px",
        "width-full": "calc(var(--wp--custom--grid--width) + var(--wp--custom--grid--gutter) * 2)",
        "width-narrow": "1000px"
      },
      "spacings": {
        "xxs": "4px",
        "xs": "8px",
        "s": "16px",
        "m": "24px",
        "l": "32px",
        "xl": "40px",
        "xxl": "48px",
        "xxxl": "56px",
        "xxxxl": "64px",
        "xxxxxl": "72px",
        "xxxxxxl": "100px"
      },
      "typography": {
        "font-body": "Figtree, sans-serif",
        "font-headings": "Figtree, sans-serif",
        "headings": {
          "h1-font-size-desktop": "5.3125rem",
          "h1-font-size-mobile": "2.25rem",
          "h1-font-weight": 700,
          "h1-line-height-desktop": 1.2,
          "h1-line-height-mobile": 1.2,
          "h2-font-size-desktop": "3.4375rem",
          "h2-font-size-mobile": "2rem",
          "h2-font-weight": 700,
          "h2-line-height-desktop": 1.29,
          "h2-line-height-mobile": 1.3125,
          "h3-font-size-desktop": "3rem",
          "h3-font-size-mobile": "1.75rem",
          "h3-font-weight": 700,
          "h3-line-height-desktop": 1.29,
          "h3-line-height-mobile": 1.29,
          "h4-font-size-desktop": "2.5rem",
          "h4-font-size-mobile": "1.5625rem",
          "h4-font-weight": 700,
          "h4-line-height-desktop": 1.3,
          "h4-line-height-mobile": 1.28,
          "h5-font-size-desktop": "2.25rem",
          "h5-font-size-mobile": "1.375rem",
          "h5-font-weight": 700,
          "h5-line-height-desktop": 1.3,
          "h5-line-height-mobile": 1.32,
          "h6-font-size-desktop": "1.75rem",
          "h6-font-size-mobile": "1.375rem",
          "h6-font-weight": 700,
          "h6-line-height-desktop": 1.2857,
          "h6-line-height-mobile": 1.32
        }
      }
    },
    "layout": {
      "contentSize": "var(--wp--custom--grid--width)",
      "wideSize": "1620px"
    },
    "spacing": {
      "customSpacingSize": false,
      "blockGap": null,
      "margin": false,
      "padding": false,
      "units": [
        "px",
        "%",
        "vh",
        "vw"
      ],
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "size": "clamp(1.5rem, 5vw, 2rem)",
          "slug": "10",
          "name": "1"
        },
        {
          "size": "clamp(2rem, 6vw, 3rem)",
          "slug": "20",
          "name": "2"
        },
        {
          "size": "clamp(2.5rem, 7vw, 4.5rem)",
          "slug": "30",
          "name": "3"
        },
        {
          "size": "clamp(3.5rem, 9vw, 6rem)",
          "slug": "40",
          "name": "4"
        }
      ]
    },
    "typography": {
      "fluid": false,
      "customFontSize": false,
      "fontWeight": false,
      "fontStyle": false,
      "lineHeight": false,
      "textTransform": false,
      "letterSpacing": false,
      "textDecoration": false,
      "dropCap": false,
      "fontFamilies": [],
      "fontSizes": [
        {
          "slug": "xx-small",
          "size": "0.75rem",
          "name": "XX Small (12px)"
        },
        {
          "slug": "x-small",
          "size": "0.875rem",
          "name": "Extra Small (14px)"
        },
        {
          "slug": "small",
          "size": "1rem",
          "name": "Small (16px)"
        },
        {
          "slug": "base",
          "size": "1.125rem",
          "name": "Base (18px)"
        },
        {
          "slug": "medium",
          "size": "1.375rem",
          "name": "Medium (22px)"
        },
        {
          "slug": "large",
          "size": "1.5rem",
          "name": "Large (24px)"
        },
        {
          "slug": "x-large",
          "size": "1.75rem",
          "name": "Extra Large (28px)"
        },
        {
          "slug": "xx-large",
          "size": "2rem",
          "name": "XX Large (32px)"
        },
        {
          "slug": "xxx-large",
          "size": "2.5rem",
          "name": "XXX Large (40px)"
        },
        {
          "slug": "xxxx-large",
          "size": "3.75rem",
          "name": "XXXX Large (60px)"
        }
      ]
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": false
        },
        "color": {
          "background": true,
          "palette": [
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "typography": {
          "fontSizes": []
        }
      },
      "core/column": {
        "border": {
          "color": true
        },
        "color": {
          "background": true,
          "custom": true,
          "palette": [
            {
              "slug": "yellow",
              "color": "var(--wp--custom--colors--yellow)",
              "name": "Yellow"
            },
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "gray-100",
              "color": "var(--wp--custom--colors--gray-100)",
              "name": "Gray"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSizes": []
        }
      },
      "core/columns": {
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSizes": []
        }
      },
      "core/cover": {
        "color": {
          "palette": [
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "spacing": {
          "padding": true
        }
      },
      "core/group": {
        "border": {
          "color": true,
          "width": true
        },
        "color": {
          "background": true,
          "custom": true,
          "palette": [
            {
              "slug": "yellow",
              "color": "var(--wp--custom--colors--yellow)",
              "name": "Yellow"
            },
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "gray-100",
              "color": "var(--wp--custom--colors--gray-100)",
              "name": "Gray"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "spacing": {
          "padding": true
        }
      },
      "core/heading": {
        "color": {
          "text": true,
          "link": true,
          "background": false,
          "palette": [
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "spacing": {
          "padding": true
        }
      },
      "core/list": {
        "color": {
          "text": true,
          "palette": [
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        }
      },
      "core/media-text": {
        "color": {
          "background": true,
          "palette": [
            {
              "slug": "gray-100",
              "color": "var(--wp--custom--colors--gray-100)",
              "name": "Gray"
            }
          ]
        }
      },
      "core/paragraph": {
        "color": {
          "text": true,
          "link": true,
          "palette": [
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        }
      },
      "core/pullquote": {
        "border": {
          "radius": false,
          "style": false,
          "color": false,
          "width": false
        },
        "color": {
          "text": true,
          "background": true,
          "palette": [
            {
              "slug": "yellow",
              "color": "var(--wp--custom--colors--yellow)",
              "name": "Yellow"
            },
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "gray-200",
              "color": "var(--wp--custom--colors--gray-200)",
              "name": "Gray"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "typography": {
          "fontSizes": []
        }
      },
      "core/quote": {
        "typography": {
          "fontSizes": []
        }
      },
      "core/separator": {
        "color": {
          "background": true,
          "palette": [
            {
              "slug": "yellow",
              "color": "var(--wp--custom--colors--yellow)",
              "name": "Yellow"
            },
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        },
        "spacing": {
          "margin": true
        }
      },
      "core/social-links": {
        "color": {
          "background": true,
          "palette": [
            {
              "slug": "yellow",
              "color": "var(--wp--custom--colors--yellow)",
              "name": "Yellow"
            },
            {
              "slug": "black",
              "color": "var(--wp--custom--colors--black)",
              "name": "Black"
            },
            {
              "slug": "white",
              "color": "var(--wp--custom--colors--white)",
              "name": "White"
            }
          ]
        }
      }
    }
  },
  "styles": {
    "elements": {
      "heading": {
        "color": {
          "text": "var(--wp--custom--colors--black)"
        }
      },
      "caption": {
        "typography": {
          "fontWeight": "500"
        }
      },
      "cite": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "400",
          "lineHeight": "1.5",
          "letterSpacing": "1px",
          "textTransform": "uppercase"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--custom--colors--black)"
        },
        "typography": {
          "textDecoration": "underline"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--custom--colors--yellow)"
          }
        },
        ":focus": {
          "color": {
            "text": "var(--wp--custom--colors--yellow)"
          }
        }
      }
    },
    "color": {
      "text": "var(--wp--custom--colors--black)",
      "background": "var(--wp--custom--colors--white)"
    },
    "typography": {
      "fontFamily": "var(--wp--custom--typography--font-body)",
      "fontSize": "1.125rem",
      "lineHeight": "1.5",
      "fontWeight": "400",
      "fontStyle": "normal"
    },
    "blocks": {
      "core/pullquote": {
        "color": {
          "background": "var(--wp--custom--colors--yellow)"
        },
        "typography": {
          "fontWeight": "800"
        }
      },
      "core/quote": {
        "color": {
          "background": "var(--wp--custom--colors--gray-100)"
        },
        "typography": {
          "fontWeight": "800"
        }
      },
      "core/separator": {
        "color": {
          "background": "var(--wp--custom--colors--gray-200)"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--20)"
          }
        }
      }
    }
  }
}
